        <servlet-class>Controller.UserStatusChangeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuizReviewController</servlet-name>
        <servlet-class>Controller.QuizReviewController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuizGenerateController</servlet-name>
