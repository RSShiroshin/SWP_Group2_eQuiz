    <servlet>
        <servlet-name>loginController</servlet-name>
        <servlet-class>Controller.loginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CourseManager</servlet-name>
        <url-pattern>/CourseManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeProfileController</servlet-name>
